{
  "name": "Note Autofill",
  "version": "0.0.0.1",
  "manifest_version": 3,
  "permissions": [ "activeTab", "scripting","storage" ],
  "action": {
    "default_icon": {
      "16": "/images/fav16.png",
      "32": "/images/fav32.png",
      "48": "/images/fav48.png",
      "96": "/images/fav96.png"
    },
    "default_popup": "popup.html",
    "default_title": "Click to show an alert"
  },
  "content_scripts": [{
    "js": ["jquery.js"],
    "matches": ["http://*/*", "https://*/*"]
   }],
  "icons": {
    "16": "/images/fav16.png",
    "32": "/images/fav32.png",
    "48": "/images/fav48.png",
    "96": "/images/fav96.png"
  }
}